{
  "name": "ifusionone",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "dist-electron/main.js",
  "description": "iFusionOne",
  "author": "mohit@ifusionone.com",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron; cross-env NODE_ENV=development electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile:electron": "tsc --project src/electron/tsconfig.json",
    "dist:mac": "npm run transpile:electron && npm run build && electron-builder --mac --arm64",
    "dist:win": "npm run transpile:electron && npm run build && electron-builder --win --x64",
    "dist:linux": "npm run transpile:electron && npm run build && electron-builder --linux --x64"
  },
  "dependencies": {
    "@langchain/community": "^0.3.38",
    "@langchain/core": "^0.3.43",
    "@langchain/langgraph": "^0.2.61",
    "@langchain/langgraph-supervisor": "^0.0.9",
    "@langchain/openai": "^0.5.2",
    "axios": "^1.8.4",
    "cheerio": "^1.0.0",
    "cross-env": "^7.0.3",
    "danfojs": "^1.1.2",
    "danfojs-node": "^1.1.2",
    "duck-duck-scrape": "^2.2.7",
    "duckduckgo-search": "^1.0.7",
    "json2csv": "^6.0.0-alpha.2",
    "langchain": "^0.3.19",
    "monaco-editor": "^0.52.2",
    "node-llama-cpp": "^3.5.0",
    "npm-run-all": "^4.1.5",
    "os-utils": "^0.0.14",
    "papaparse": "^5.5.2",
    "parquetjs-lite": "^0.8.7",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.50.0",
    "plotly.js-dist-min": "^3.0.1",
    "react": "^18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-flow-renderer": "^10.3.17",
    "react-icons": "^5.4.0",
    "react-markdown": "^10.0.1",
    "react-router-dom": "^7.1.3",
    "reactflow": "^11.11.4",
    "recharts": "^2.15.1",
    "simple-statistics": "^7.8.8",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "depcheck": "^1.4.7",
    "electron": "^34.0.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
